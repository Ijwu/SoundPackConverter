<Window x:Class="SoundPackConverter.ConfigEditor.WPF.EditorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoundPackConverter.ConfigEditor.WPF"
        mc:Ignorable="d"
        Title="EditorWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <ListBox ItemsSource="{Binding SoundNames}"></ListBox>
        </StackPanel>
    </Grid>
</Window>
